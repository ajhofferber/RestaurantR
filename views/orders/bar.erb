<h2> BAR ORDERS</h2>


<%@orders.each do |order|%>

 <%if order.item.category == "drinks"%>
 <% if order.status == 'open' %>
  <h5 class = 'order' data-start-time= "<%=order.created_at%>">  Table Number: <%= order.party.table_number%> - <%= order.item.name %> </h5>

  <form action="/orders/bar/<%=order.id%>" method="post">
    <input type="submit" value="DRINKS UP">
    </form>
    <%end%>
 <%end%>
<%end%>
